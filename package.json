{
  "name": "cocolani-app",
  "version": "1.0.6",
  "build": {
  "appId": "cocolani.app",
  "productName": "Cocolani Islands",
  "asar": true,
  "asarUnpack": [
    "**/pepflashplayer.dll",
    "**/libpepflashplayer.so",
    "**/PepperFlashPlayer.plugin"
  ],
  "extraResources": [
    {
      "from": "flash/",
      "to": "flash"
    }
  ],
  "mac": {
    "target": "dmg",
    "icon": "images/applogo.png"
  },
  "win": {
    "target": "nsis",
    "icon": "images/applogo.png"
  },
  "publish": {
    "provider": "github",
    "owner": "medr32",
    "repo": "cocolani-app"
  }
},
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "dotenv -e .env -- electron-builder",
    "publish": "dotenv -e .env -- electron-builder --publish always"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "Desktop Application for the game Cocolani Islands.",
  "devDependencies": {
    "dotenv-cli": "^10.0.0",
    "electron": "^11.5.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "discord-rpc": "^4.0.1",
    "dotenv": "^10.0.0",
    "electron-updater": "^6.3.9"
  }
}
